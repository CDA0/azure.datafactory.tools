							"type": "Query",
							"text": "update STAGE_CDI.BATCH_PARAMETER\n   set PARAMETER_VALUE_DT = case \n                              when (select max(LASTMODIFIEDDATE) \n                                     from STAGE_LOAD.TD_SFDC_ADVISER_ACCOUNT_STG) is not null then \n                                (select max(LASTMODIFIEDDATE) \n                                  from STAGE_LOAD.TD_SFDC_ADVISER_ACCOUNT_STG) \n                            else \n                              PARAMETER_VALUE_DT \n                            end,\n       LASTMODIFIEDDT = (select cast(SYSDATETIMEOFFSET() at time zone 'GMT Standard Time' as datetime)),\n       LASTMODIFIEDBY = 'p_SFDC-CDI-SFDC_ADVISER_ACCOUNT_STG'\n where PROCESS_NAME = 'p_SFDC-CDI-SFDC_ADVISER_ACCOUNT_STG'\n   and PARAMETER_NAME = 'prm_EXTRACTDT'"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
