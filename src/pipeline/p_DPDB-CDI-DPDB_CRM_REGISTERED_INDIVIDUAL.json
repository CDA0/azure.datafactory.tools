						"sqlReaderQuery": {
							"value": "select TDRIS.ACCOUNT_NUMBER,\n\t     TDRIS.PARTY_ROLE_ID,    \n       TDRIS.OFFICE_PARTY_ROLE_ID,\n       TDRIS.FIRM_PARTY_ROLE_ID,\n       TDRIS.REL_OFFICE_PARTY_ROLE_ID,\n       TDRIS.REL_FIRM_PARTY_ROLE_ID,\n       TDRIS.TITLE_ID,\n       TDRIS.FORENAME,\n       TDRIS.SURNAME,\n       TDRIS.INDIVIDUAL_REFERENCE_NUMBER,\n       TDRIS.STATUS,\n       TDRIS.FIRM_REFERENCE_NUMBER,\n       TDRIS.ACCOUNT_STATUS,\n       TDRIS.JOB_TITLE_ID,\n       TDRIS.ADDRESS_LINE_1,\n       TDRIS.ADDRESS_LINE_2,\n       TDRIS.ADDRESS_LINE_3,\n       TDRIS.ADDRESS_LINE_4,\n       TDRIS.POSTCODE,\n       TDRIS.ACCOUNT_TYPE_ID ,\n       TDRIS.LEGACY_SYSTEM_ID,\n       TDRIS.RECORD_UPDATE_IND,\n       cast(0 as bit) as RECORD_DELETE_IND,\n       cast(sysdatetimeoffset() at time zone 'GMT Standard Time' as datetime) as UPDATE_DATE_TIME\n  from (select ACCOUNT_NUMBER,\n               PARTY_ROLE_ID, \n               OFFICE_PARTY_ROLE_ID,\n               FIRM_PARTY_ROLE_ID,\n               REL_OFFICE_PARTY_ROLE_ID,\n               REL_FIRM_PARTY_ROLE_ID,\n               TITLE_ID,\n               FORENAME,\n               SURNAME,\n               INDIVIDUAL_REFERENCE_NUMBER,\n               STATUS,\n               FIRM_REFERENCE_NUMBER,\n               ACCOUNT_STATUS,\n               JOB_TITLE_ID,\n               ADDRESS_LINE_1,\n               ADDRESS_LINE_2,\n               ADDRESS_LINE_3,\n               ADDRESS_LINE_4,\n               POSTCODE,\n               ACCOUNT_TYPE_ID ,\n               LEGACY_SYSTEM_ID,\n               convert(nvarchar(32), HashBytes('SHA2_256', concat(ACCOUNT_NUMBER,\n                                                                  PARTY_ROLE_ID, \n                                                                  OFFICE_PARTY_ROLE_ID,\n                                                                  FIRM_PARTY_ROLE_ID,\n                                                                  REL_OFFICE_PARTY_ROLE_ID,\n                                                                  REL_FIRM_PARTY_ROLE_ID,\n                                                                  TITLE_ID,\n                                                                  FORENAME,\n                                                                  SURNAME,\n                                                                  INDIVIDUAL_REFERENCE_NUMBER,\n                                                                  STATUS,\n                                                                  FIRM_REFERENCE_NUMBER,\n                                                                  ACCOUNT_STATUS,\n                                                                  JOB_TITLE_ID,\n                                                                  ADDRESS_LINE_1,\n                                                                  ADDRESS_LINE_2,\n                                                                  ADDRESS_LINE_3,\n                                                                  ADDRESS_LINE_4,\n                                                                  POSTCODE,\n                                                                  ACCOUNT_TYPE_ID ,\n                                                                  LEGACY_SYSTEM_ID)), 2) as RECORD_UPDATE_IND\n         from STAGE_LOAD.TD_DPDB_REGISTERED_INDIVIDUAL_STG \n        ) TDRIS\n  left outer join STAGE_CDI.DPDB_REGISTERED_INDIVIDUAL_STG DRIS\n    on (    TDRIS.ACCOUNT_NUMBER = DRIS.ACCOUNT_NUMBER\n        and TDRIS.RECORD_UPDATE_IND = DRIS.RECORD_UPDATE_IND)\n where DRIS.ACCOUNT_NUMBER is null",
