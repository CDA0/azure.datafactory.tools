{
	"name": "p_SFDC-CDI-SFDC_PLAT_DYN_ACTIVITY_POINTER_STG",
	"properties": {
		"description": "Author: Jilly Webster\nDate: 10/07/2023\nDescription: ASCD-20271 - this pipeline is loading data data from Task object of Salesforce to SFDC_PLAT_DYN_ACTIVITY_POINTER_STG table in Stage CDI.",
		"activities": [
			{
				"name": "cd-SFDC_PLAT_DYN_ACTIVITY_POINTER_STG",
				"description": "pulls data from Salesforce ActivityPointer object and copies to the STAGE_CDI table SFDC_PLAT_DYN_ACTIVITY_POINTER_STG using SQL query",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "sp-TRUNCATE-SFDC_PLAT_DYN_ACTIVITY_POINTER_STG",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SalesforceSource",
						"additionalColumns": [
							{
								"name": "ORIGIN",
								"value": {
									"value": "'MSDynamics_Migration'",
									"type": "Expression"
								}
							}
						],
