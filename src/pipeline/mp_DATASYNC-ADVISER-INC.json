					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "ls_AzureSQLDB",
					"type": "LinkedServiceReference",
					"parameters": {
						"ENVIRONMENT": "@pipeline().globalParameters.ENVIRONMENT",
						"SHORT_LOCATION": "@pipeline().globalParameters.SHORT_LOCATION",
						"LOGICAL_ENVIRONMENT": "@pipeline().globalParameters.LOGICAL_ENVIRONMENT"
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "select case\n         when sum(RW.RECORDS_WRITTEN) > 0 then\n           1\n        else\n          0\n        end as PROCESS_PIPELINES\n  from (select case\n                when RECORDS_WRITTEN > 0 then\n                    1\n                else\n                    0\n               end as RECORDS_WRITTEN\n          from STAGE_CDI.BATCH_AUDIT\n         where TABLE_NAME = 'DPDB_ADVISER_FIRM_STG'\n           and PIPELINE_NAME = 'p_DPDB-CDI-DPDB_ADVISER_FIRM_STG'\n         union all\n        select case\n                when RECORDS_WRITTEN > 0 then\n                1\n                else\n                0\n               end as RECORDS_WRITTEN\n          from STAGE_CDI.BATCH_AUDIT\n         where TABLE_NAME = 'DPDB_ADVISER_OFFICE_STG'\n           and PIPELINE_NAME = 'p_DPDB-CDI-DPDB_ADVISER_OFFICE_STG'\n         union all\n        select case\n                when RECORDS_WRITTEN > 0 then\n                1\n                else\n                0\n               end as RECORDS_WRITTEN\n          from STAGE_CDI.BATCH_AUDIT\n         where TABLE_NAME = 'DPDB_REGISTERED_INDIVIDUAL_STG'\n           and PIPELINE_NAME = 'p_DPDB-CDI-DPDB_CRM_REGISTERED_INDIVIDUAL_STG'\n         union all\n        select case\n                when RECORDS_WRITTEN > 0 then\n                1\n                else\n                0\n               end as RECORDS_WRITTEN\n          from STAGE_CDI.BATCH_AUDIT\n         where TABLE_NAME = 'DPDB_ADVISER_NETWORK_STG'\n           and PIPELINE_NAME = 'p_DPDB-CDI-DPDB_ADVISER_NETWORK_STG'\n         union all\n        select case\n                when RECORDS_WRITTEN > 0 then\n                1\n                else\n                0\n               end as RECORDS_WRITTEN\n          from STAGE_CDI.BATCH_AUDIT\n         where TABLE_NAME = 'DPDB_ADVISER_LEGACY_AGENCY_STG'\n           and PIPELINE_NAME = 'p_DPDB-CDI-DPDB_ADVISER_LEGACY_AGENCY_STG'\n        ) RW"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
