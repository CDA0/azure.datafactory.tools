{
    "name": "ls_SFDC_ENIGMA",
    "properties": {
        "description": "Connects to Salesforce Wealth Operations Org using adfenigmaintegrationuser account",
        "parameters": {
            "ENVIRONMENT": {
                "type": "string"
            },
            "LOGICAL_ENVIRONMENT": {
                "type": "string"
            },
            "SHORT_LOCATION": {
                "type": "string"
            },
            "SFDC_ENIGMA_URL": {
                "type": "string"
            },
            "SFDC_ENIGMA_USERNAME": {
                "type": "string"
            },
            "SFDC_ENIGMA_API_VERSION": {
                "type": "string"
            },
            "SFDC_ENIGMA_ORG": {
                "type": "string"
            }
        },
        "annotations": [],
        "type": "Salesforce",
        "typeProperties": {
            "environmentUrl": "@{linkedService().SFDC_ENIGMA_URL}",
            "username": "@{linkedService().SFDC_ENIGMA_USERNAME}",
            "password": {
                "type": "AzureKeyVaultSecret",
                "store": {
                    "referenceName": "ls_KeyVaultCom",
                    "type": "LinkedServiceReference",
                    "parameters": {
                        "ENVIRONMENT": {
                            "value": "@linkedService().ENVIRONMENT",
                            "type": "Expression"
                        },
                        "SHORT_LOCATION": {
                            "value": "@linkedService().SHORT_LOCATION",
                            "type": "Expression"
                        }
                    }
                },
                "secretName": {
                    "value": "SFDC-adfenigmaintegrationuser-@{linkedService().SFDC_ENIGMA_ORG}",
                    "type": "Expression"
                }
            },
            "apiVersion": "@{linkedService().SFDC_ENIGMA_API_VERSION}"
        },
        "connectVia": {
            "referenceName": "Woking",
            "type": "IntegrationRuntimeReference"
        }
    },
    "type": "Microsoft.DataFactory/factories/linkedservices"
}