{
	"name": "ls_DPDB",
	"properties": {
		"description": "Connection for DPDB Oracle database",
		"parameters": {
			"ENVIRONMENT": {
				"type": "string"
			},
			"LOGICAL_ENVIRONMENT": {
				"type": "string"
			},
			"SHORT_LOCATION": {
				"type": "string"
			},
			"DPDB": {
				"type": "string"
			},
			"DPDB_HOST": {
				"type": "string"
			},
			"DPDB_PORT": {
				"type": "string"
			},
			"DPDB_USERNAME": {
				"type": "string"
			},
			"DPDB_SECRET_NAME": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Oracle",
		"typeProperties": {
			"connectionString": "host=@{linkedService().DPDB_HOST};port=@{linkedService().DPDB_PORT};serviceName=@{linkedService().DPDB};user id=@{linkedService().DPDB_USERNAME}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_KeyVaultCom",
					"type": "LinkedServiceReference",
					"parameters": {
						"ENVIRONMENT": {
							"value": "@linkedService().ENVIRONMENT",
							"type": "Expression"
						},
						"SHORT_LOCATION": {
							"value": "@linkedService().SHORT_LOCATION",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@{linkedService().DPDB_SECRET_NAME}",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "Lansing",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}