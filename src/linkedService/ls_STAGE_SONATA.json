{
	"name": "ls_STAGE_SONATA",
	"properties": {
		"description": "Connects to STAGE_SONTA via the STAGE_SONATA_IICS username",
		"parameters": {
			"ENVIRONMENT": {
				"type": "string"
			},
			"SHORT_LOCATION": {
				"type": "string"
			},
			"LOGICAL_ENVIRONMENT": {
				"type": "string"
			},
			"STAGE_SONATA_DB": {
				"type": "string"
			},
			"STAGE_SONATA_DB_HOST": {
				"type": "string"
			},
			"STAGE_SONATA_DB_PORT": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Oracle",
		"typeProperties": {
			"connectionString": "host=@{linkedService().STAGE_SONATA_DB_HOST};port=@{linkedService().STAGE_SONATA_DB_PORT};sid=@{linkedService().STAGE_SONATA_DB};user id=stage_sonata_adf",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_KeyVaultCom",
					"type": "LinkedServiceReference",
					"parameters": {
						"ENVIRONMENT": {
							"value": "@linkedService().ENVIRONMENT",
							"type": "Expression"
						},
						"SHORT_LOCATION": {
							"value": "@linkedService().SHORT_LOCATION",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@{linkedService().LOGICAL_ENVIRONMENT}-@{linkedService().STAGE_SONATA_DB}-STAGE-SONATA-ADF",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "Lansing",
			"type": "IntegrationRuntimeReference"
		}
	}
}