{
	"name": "ls_OCDB",
	"properties": {
		"description": "Connects to OCDB using the OCDB01P_CDI_ADF account",
		"parameters": {
			"ENVIRONMENT": {
				"type": "string"
			},
			"LOGICAL_ENVIRONMENT": {
				"type": "string"
			},
			"SHORT_LOCATION": {
				"type": "string"
			},
			"OCDB": {
				"type": "string"
			},
			"OCDB_HOST": {
				"type": "string"
			},
			"OCDB_PORT": {
				"type": "string"
			},
			"OCDB_USERNAME": {
				"type": "string"
			},
			"OCDB_SECRET_NAME": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Oracle",
		"typeProperties": {
			"connectionString": "host=@{linkedService().OCDB_HOST};port=@{linkedService().OCDB_PORT};sid=@{linkedService().OCDB};user id=@{linkedService().OCDB_USERNAME}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_KeyVaultCom",
					"type": "LinkedServiceReference",
					"parameters": {
						"ENVIRONMENT": {
							"value": "@linkedService().ENVIRONMENT",
							"type": "Expression"
						},
						"SHORT_LOCATION": {
							"value": "@linkedService().SHORT_LOCATION",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@{linkedService().OCDB_SECRET_NAME}",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "Lansing",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}