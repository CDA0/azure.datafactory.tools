ALTER TABLE STAGE_LOAD.TD_CRM_PLAT_DYN_INCIDENT_STG 
  ADD CONSTRAINT DF_TD_CRM_PLAT_DYN_INCIDENT_STG_S_CREATE_DATETIME  DEFAULT (CONVERT(DATETIME2,(SYSDATETIMEOFFSET() AT TIME ZONE 'GMT STANDARD TIME'))) FOR S_CREATE_DATETIME;

ALTER TABLE STAGE_LOAD.TD_CRM_PLAT_DYN_INCIDENT_STG 
  ADD CONSTRAINT DF_TD_CRM_PLAT_DYN_INCIDENT_STG_S_UPDATE_DATETIME  DEFAULT (CONVERT(DATETIME2,(SYSDATETIMEOFFSET() AT TIME ZONE 'GMT STANDARD TIME'))) FOR S_UPDATE_DATETIME;

ALTER TABLE STAGE_LOAD.TD_CRM_PLAT_DYN_INCIDENT_STG
  ADD CONSTRAINT TD_CRM_PLAT_DYN_INCIDENT_STG_PK PRIMARY KEY (INCIDENTID);